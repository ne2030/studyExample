{
    "extends": "airbnb-base",
    "plugins": ["fp"],
    "env": {
        "browser": true,
        "node": true,
        "mocha": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "globals": {
        "L": true,
        "produce": true,
        "FP": true,
        "El": true
    },
    "rules": {
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "semi": ["error", "always"],
        "strict": 0,
        "eqeqeq": 0,
        "no-multi-assign": 0,
        "no-param-reassign": 0,
        "max-len": 0,
        "camelcase": 0,
        "import/prefer-default-export": 0,
        "no-fallthrough": 0,
        "no-underscore-dangle": 0,
        "arrow-body-style": 0,
        "one-var": 0,
        "consistent-return": 0,
        "no-nested-ternary": 0,
        "no-use-before-define": [
            "error",
            {
                "functions": false
            }
        ],
        "no-shadow": 0,
        "no-inner-declarations": 0,
        "one-var-declaration-per-line": 0,
        "no-restricted-properties": 0,
        "no-unused-vars": [
            "error",
            {
                "args": "none"
            }
        ],
        "quotes": [
            "error",
            "single",
            {
                "allowTemplateLiterals": true
            }
        ],
        "prefer-template": 0,
        "no-console": 0,
        "radix": 0,
        "func-names": 0,
        "comma-dangle": 0
    }
}
